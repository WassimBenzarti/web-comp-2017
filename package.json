{
  "name": "client",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start2": "ng serve",
    "start": "npm run tsc && npm run moveassets && concurrently \"npm run lite-server\" \"tsc -w\" \"npm run watchassets\" \"npm run livereload\" ",
    "livereload": "livereload dist -p 8081 -e css -d",
    "postinstall": "npm run tsc && npm run moveassets",
    "tsc": "tsc",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "moveassets": "rsync -a --include='*.css' --include='*.html' --include='*/' --exclude='*' ./app/ ./dist/",
    "watchassets": "onchange \"app/**/*.css\" \"app/**/*.html\" -v -- npm run moveassets",
    "lite-server":"lite-server -p 8080"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^2.3.1",
    "@angular/compiler": "^2.3.1",
    "@angular/core": "^2.3.1",
    "@angular/forms": "^2.3.1",
    "@angular/http": "^2.3.1",
    "@angular/platform-browser": "^2.3.1",
    "@angular/platform-browser-dynamic": "^2.3.1",
    "@angular/router": "^3.3.1",
    "@types/node": "^6.0.42",
    "core-js": "^2.4.1",
    "rxjs": "^5.0.1",
    "systemjs": "0.19.40",
    "ts-node": "1.2.1",
    "typescript": "~2.0.3",
    "zone.js": "^0.7.4"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^2.3.1",
    "@types/jasmine": "2.5.38",
    "angular-cli": "1.0.0-beta.24",
    "codelyzer": "~2.0.0-beta.1",
    "concurrently": "^3.1.0",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "lite-server": "^2.2.2",
    "livereload": "^0.6.0",
    "onchange": "^3.2.1",
    "protractor": "~4.0.13",
    "reload": "^1.1.0",
    "ts-helpers": "^1.1.1",
    "tslint": "^4.0.2"
  }
}
